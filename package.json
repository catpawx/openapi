{
  "name": "@catpawx/openapi",
  "version": "1.0.1",
  "description": "🐾 猫爪联盟前端团队自动化生成接口",
  "bin": {
    "cpx-openapi": "dist/index.js"
  },
  "keywords": [
    "cli",
    "template"
  ],
  "files": [
    "dist"
  ],
  "authors": [
    "相甫 <xiangfu_git@163.com>"
  ],
  "homepage": "https://github.com/catpawx/@catpawx/cli-template#readme",
  "license": "ISC",
  "scripts": {
    "openapi2ts": "openapi2ts",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "commit": "git-cz",
    "build": "npm run lint && npm run prettier && tsc --build",
    "lint": "eslint --fix src",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint-staged": "lint-staged --allow-empty",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@catpawx/commitlint-config-preset": "^0.1.0",
    "@catpawx/eslint-config-preset": "^2.0.0",
    "@catpawx/prettier-config-preset": "^2.0.0",
    "@changesets/cli": "^2.27.6",
    "@commitlint/cli": "^19.3.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.10.2",
    "@types/prompts": "^2.4.9",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^8.12.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "minimist": "^1.2.8",
    "@umijs/openapi": "^1.13.0"
  }
}
